<table class="table tableborder">

  <thead>
    <tr>
      <th scope="col">#</th>

      <th scope="col">Postulantes</th>
      <th scope="col">Dni</th>
      <th scope="col">Telefono</th>
      <th scope="col">Email</th>
      <th scope="col"></th>
      
    </tr>
  </thead>

  @foreach (var item in ViewBag.PostulantesList)
  {
    <tbody>
      <tr>
        <th scope="row">@item.ID</th>

        <td>@item.Nombre</td>
        <td>@item.DNI</td>
        <td>@item.Telefono</td>
        <td>@item.Email</td>
        <td>
          <button type="button" class="btn btn-warning"><a asp-action="Editar" asp-controller="Postulantes"
          class="text-light not"  asp-route-id="@item.ID">Edit</a></button>

          <button type="button" class="btn btn-danger"><a asp-action="Eliminar" asp-controller="Postulantes"
          class="text-light not"  asp-route-id="@item.ID">Delete</a></button>

          <button type="button" class="btn btn-success"><a class="text-light not">Details</a></button>
        </td>

      </tr>
    </tbody>
  }

</table>
<button type="button" class="btn btn-primary"><a class="text-light not" asp-action="ConsultarEmpresas"
    asp-controller="Empresas">Consultar empresas</a></button>
