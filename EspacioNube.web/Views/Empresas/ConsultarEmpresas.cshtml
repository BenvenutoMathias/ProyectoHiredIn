@* @model List<Empresa> *@


<table class="table">

  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Empresas</th>
      <th scope="col">Rubro</th>
      <th scope="col">Telefono</th>
      <th scope="col">Email</th>
      <th scope="col"></th>
      @* <th scope="col">Postulantes</th> *@
    </tr>
  </thead>

@foreach (var item in ViewBag.EmpresasList  @* @Model *@)
{
  <tbody>
    <tr>
      <th scope="row">@item.ID</th>
      <td>@item.NombreEmpresa</td>
      <td>@item.Rubro</td>
      <td>@item.Telefono</td>
      <td>@item.Email</td>
      <td><button type="button" class="btn btn-warning">@* <a asp-action="Editar" asp-controller="Empresas" asp-route-id="@item.ID">Editar</a> *@Edit</button>
      <button type="button" class="btn btn-danger">@* <a asp-action="Eliminar" asp-controller="Empresas" asp-route-id="@item.ID">Delete</a> *@Delete</button></td>
      @* <td>Juan Carlos</td> *@
    </tr>
  </tbody>
}

</table>

<button type="button" class="btn btn-primary"><a class="nav-link text-light" asp-action="ConsultarPostulantes" asp-controller="Postulantes">Consultar Postulantes</a></button>



        


    
